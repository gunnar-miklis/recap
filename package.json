{
  "name": "recap",
  "packageManager": "yarn@4.3.1",
  "type": "module",
  "scripts": {
    "dev": "tsc --watch",
    "build:dev": "tsc --noEmit false",
    "format:check": "prettier --check src/**/*",
    "format:fix": "prettier --write src/**/*",
    "lint:check": "eslint \"src/**/*.{js,jsx,ts,tsx}\"",
    "lint:fix": "eslint \"src/**/*.{js,jsx,ts,tsx}\" --fix",
    "startBackend1": "nodemon ./src/backend/crud-db/server.js",
    "startBackend2": "nodemon ./src/backend/pw-encryption/server.js",
    "startBackend3": "nodemon ./src/full-stack/jwt-auth/server/server.js",
    "startBackend4": "nodemon ./src/full-stack/file-upload/server/server.js",
    "startFrontend1": "vite src/frontend/react-demo",
    "startFrontend2": "vite src/frontend/routing",
    "startFrontend3": "vite src/frontend/react-context",
    "startFrontend4": "vite src/full-stack/jwt-auth/client",
    "startFrontend5": "vite src/full-stack/file-upload/client",
    "startJsonServer": "json-server --watch src/backend/json-server-fake-api/fake-blog.json --port 8000"
  },
  "dependencies": {
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "express-jwt": "^8.4.1",
    "jsonwebtoken": "^9.0.2",
    "mongoose": "^8.3.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@vitejs/plugin-react": "^4.2.1",
    "axios": "^1.6.8",
    "bcryptjs": "^2.4.3",
    "eslint": "^8.49.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-react": "^7.34.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.6",
    "json-server": "^1.0.0-alpha.23",
    "morgan": "^1.10.0",
    "nodemon": "^3.1.0",
    "prettier": "3.0.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.22.3",
    "vite": "^5.2.10"
  }
}
